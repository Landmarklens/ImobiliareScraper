# ImobiliareScraper Environment Variables Example
# Copy this file to .env and fill with your values

# Environment
ENVIRONMENT=development
DEV_MODE=true

# Database
DB_CONNECTION_STRING=postgresql://username:password@localhost:5432/homeai_db

# AWS Services (optional for local development)
S3_ACCESS_KEY=your_s3_access_key
S3_SECRET_KEY=your_s3_secret_key
S3_BUCKET_NAME=homeai-scraped-data

# Geocoding Services
OPEN_CAGE_API_KEY=your_opencage_api_key
GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# Proxy Configuration (optional)
PROXY_ENABLED=false
WEBSHARE_API_KEY=your_webshare_api_key
PROXY_REFRESH_HOURS=3.0

# Monitoring
SCRAPEOPS_API_KEY=your_scrapeops_api_key
SLACK_WEBHOOK_URL=your_slack_webhook

# Romania Specific Settings
IMOBILIARE_API_KEY=
IMOBILIARE_RATE_LIMIT=1.0
IMOBILIARE_USER_AGENT=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36